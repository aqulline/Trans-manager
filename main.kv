#:import sm kivy.uix.screenmanager
ScreenManager:
    id:manager
    transition:sm.FadeTransition(duration=.0)
    MDScreen:
        name:"dashboard"
        md_bg_color:.9, .9, .95, 1
        FloatLayout:
            CashCard:
                size: 1920, 220
                radius:[25]
                pos_hint:{"center_y":.95, "center_x":.5}
                elevation:0
                md_bg_color: .94, .94, .99, 1
                focus_behavior: False
                ripple_behavior: False
            CashCard:
                size: 300, 100
                radius:[25]
                pos_hint:{"center_y":.93, "center_x":.92}
                elevation:3
                MDIcon:
                    icon:"account"
                    halign:"center"
                    font_size:sp(40)
                    #size_hint:None, None
                    pos_hint:{"center_y":.5, "center_x":.5}
                    theme_text_color:"Custom"
                    text_color:150/255, 111/255, 51/255, 1
                MDLabel:
                    halign:"center"
                    text:"Peter Kuwanda"
                    font_size:sp(25)
                    font_name:'components/fonts/Gill Sans.otf'
                    pos_hint:{"center_y":.5, "center_x":.5}
                    #size_hint:None, None
                    theme_text_color:"Custom"
                    text_color:150/255, 111/255, 51/255, 1
            MDLabel:
                halign:"center"
                text:"jul 25"
                font_style:"Body1"
                font_size:sp(25)
                font_name:'components/fonts/Gill Sans.otf'
                pos_hint:{"center_y":.98, "center_x":.5}
                theme_text_color:"Custom"
                text_color:150/255, 111/255, 51/255, 1
            MDLabel:
                halign:"center"
                text:"Dashboard"
                font_style:"H3"
                font_name:'components/fonts/Gill Sans.otf'
                pos_hint:{"center_y":.92, "center_x":.1}
                theme_text_color:"Custom"
                text_color:150/255, 111/255, 51/255, 1

            CashCard:
                pos_hint:{"center_x":.2, "center_y":.65}
                Image:
                    source:"components/icons/fleet5.png"
                    size_hint:.7, .7
                    pos_hint:{"center_x":.1, "center_y":.5}
                MDLabel:
                    text:"Fleet"
                    halign:"center"
                    font_style:"H4"
                    font_size:sp(55)
                    font_name:'components/fonts/Gill Sans.otf'
                    theme_text_color:"Custom"
                    text_color:150/255, 111/255, 51/255, 1
                    pos_hint:{"center_x":.3, "center_y":.5}
            CashCard:
                pos_hint:{"center_x":.5, "center_y":.65}
                FitImage:
                    source:"components/icons/safari2.png"
                    size_hint:.6, .6
                    pos_hint:{"center_x":.3, "center_y":.5}
                MDLabel:
                    text:"Safari"
                    halign:"center"
                    font_style:"H4"
                    font_size:sp(55)
                    font_name:'components/fonts/Gill Sans.otf'
                    theme_text_color:"Custom"
                    text_color:150/255, 111/255, 51/255, 1
                    pos_hint:{"center_x":.1, "center_y":.5}
            CashCard:
                pos_hint:{"center_x":.8, "center_y":.65}
                FitImage:
                    source:"components/icons/maint.png"
                    size_hint:.5, .5
                    pos_hint:{"center_x":.45, "center_y":.5}
                MDLabel:
                    text:"Maintenance"
                    halign:"center"
                    font_style:"H6"
                    font_size:sp(43)
                    font_name:'components/fonts/Gill Sans.otf'
                    theme_text_color:"Custom"
                    text_color:150/255, 111/255, 51/255, 1
                    pos_hint:{"center_x":.45, "center_y":.5}
            MDLabel:
                halign:"center"
                text:"Summary"
                font_style:"H4"
                font_name:'components/fonts/Gill Sans.otf'
                pos_hint:{"center_y":.45, "center_x":.1}
                theme_text_color:"Custom"
                text_color:150/255, 111/255, 51/255, 1


<CashCard@MDCard>:
    #orientation: "vertical"
    md_bg_color: .9, .9, .95, 1
    padding: "10dp"
    #spacing: "5dp"
    border_radius: 20
    #radius:dp(19)
    border_color_a: .5
    radius: [15]
    size_hint: None, None
    size: 350, 220
    focus_behavior: True
    unfocus_color: .9, .9, .95, 1
    focus_color: .86, .86, .91, 1
    ripple_color:.9, .9, .95, 1
    ripple_behavior: True
    elevation: 3